
{
  "manifest_version": 2,
  "name": "Better F1TV",
  "version": "0.2",
  "description": "Better F1 Video",
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["js/jquery.js", "js/f1.js"],
      "css": [
        "style.css"
      ],
      "matches": [
        "https://f1tv.formula1.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": { "16": "icons/icon16.png",
           "48": "icons/icon48.png",
          "128": "icons/icon128.png" }
}
